{
  "rewrites": [
    {
      "source": "/((?!sitemap\\.xml).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        { "key": "Content-Type", "value": "application/xml; charset=utf-8" },
        { "key": "Content-Encoding", "value": "identity" }
      ]
    }
  ]
}
